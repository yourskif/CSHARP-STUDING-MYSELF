@model IEnumerable<dynamic>

<!DOCTYPE html>
<html lang="uk">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Головна сторінка</title>
</head>
<body>
    <h2>Доступні дії:</h2>
    <ul>
        @foreach (var item in Model)

        {
            <li>
                <a href="@item.Url">@item.Text</a>
            </li>
        }
    </ul>
</body>
</html>
